#include<stdio.h>
int main()
{
	int a[100],n,i,j;
	printf("enter size here:");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		printf("enter element:");
		scanf("%d",&a[i]);
	}
	printf("elements in the array are:");
	for(i=0;i<n;i++)
		{printf("%d \t",a[i]);}
	for(i=0;i<n;i++)
		{for(j=0;j<n;j++)
			{
				if(a[i]+a[j]==50)
				{
					printf("(%d,%d)",a[i],a[j]);
				}
			}
		}
	return 0;
}
