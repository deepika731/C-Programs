#include<stdio.h>
int main()
{
	float a,b,c,d,e,f;
	
	printf("enter first side:");
	scanf("%f",&a);
	printf("enter second side:");
	scanf("%f",&b);
	printf("enter third side:");
	scanf("%f",&c);
	d=(b*b==a*a+c*c);
	e=(a*a==b*b+c*c);
	f=(c*c==a*a+b*b);
	if(a==b && b==c)
		{printf("it is equilateral triangle");}
	else if(a==b || b==c || c==a)
		{printf("it is iscoseles triangle");
		if(d || e || f)
				{printf("it is right angled triangle");}}
	else if(a!=b && b!=c)
		{printf("it is scalane triangle");
		if(d || e || f)
				{printf("it is right angled triangle");}}
	else
		{printf("enter correct option");}
	return 0;
}
		
	
